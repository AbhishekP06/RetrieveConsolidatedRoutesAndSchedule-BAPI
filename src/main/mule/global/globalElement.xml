    <ee:object-store-caching-strategy name="Caching_Strategy" doc:name="Caching Strategy" doc:id="c2ee6b13-40c0-4732-9c51-f5fa6a05bd5d" objectStore="Object_store" keyGenerationExpression='#[vars.transportCompany ++ vars.transportType ++ "Routes"]'/>
	
	
