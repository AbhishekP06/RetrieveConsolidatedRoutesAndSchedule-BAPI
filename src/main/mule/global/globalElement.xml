    <apikit:config name="retrieveconsolidatedroutesandschedulexpapi-config" api="retrieveconsolidatedroutesandschedulexpapi.raml" outboundHeadersMapName="outboundHeaders" httpStatusVarName="httpStatus" />
    
    
	<configuration-properties doc:name="Configuration properties" doc:id="6ff6e48a-695c-4596-ba97-87c54b656ee4" file="config/${MULE_ENV}.yaml" />
	<http:request-config name="HTTP_Request_configuration_TravelOnTip-SAPI" doc:name="HTTP Request configuration" doc:id="7a3f3165-7de2-4062-9e50-38321bbfbf2c" >
		<http:request-connection host="${http.requester.travelOnTip.host}" port="${http.requester.travelOnTip.port}" />
	</http:request-config>
	<http:request-config name="HTTP_Request_configuration_FastGo-SAPI" doc:name="HTTP Request configuration" doc:id="3a56dd6b-180e-444f-a25b-229e4dedaa84" >
		<http:request-connection host="${http.requester.fastGo.host}" port="${http.requester.fastGo.port}" />
	</http:request-config>
